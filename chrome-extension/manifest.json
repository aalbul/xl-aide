{
  "name": "XL Aide",
  "description": "XL Aide in Chrome",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "assets/xl-aide.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
